/* pages/categorys.wxss */
page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row input {
  background-color: #fff;
  flex: 1;
  width:60%;
  height: 100%;
  padding-left: 30rpx;
}
.search_row button {
  font-size: 28rpx;
  padding: 30rpx;
  margin: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
